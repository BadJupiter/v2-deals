{
	"biz_id": "colusacasino",
	"check_in": true,
	"auth_required": true,
	"error_auth": {
		"title": "Woops!",
		"content": "Can't play if you ain't authenticated!"
	},

	"FORM": {
		"fields": [
			{
				"type": "info",
				"markdown": "# Fragrance Event RSVP"
			},
			{
				"type": "info",
				"markdown": "## **February 7th & 8th**"
			},
			{
				"type": "info",
				"markdown": "---"
			},
			{
				"id": "attend",
				"label": "Are you able to attend?",
				"type": "select",
				"range": ["Yes","No"],
				"required": true
			},
			{
				"id": "day",
				"label": "I am confirming for:",
				"type": "select",
				"range": ["Saturday, February 7th | 2pm - 8pm","Sunday, February 8th | 12pm - 6pm"],
				"required": true,
				"conditional": {
					"field": "attend",
					"value": "Yes"
				}
			},
			{
				"type": "info",
				"markdown": "---"
			},
			{
				"type": "fields_group",
				"bgcolor": "#c8f0fc",
				"fields": [
					{
						"id": "z_FIRSTNAME",
						"label": "First Name:",
						"type": "text",
						"sticky": true,
						"disabled": true
					},
					{
						"id": "z_LASTNAME",
						"label": "Last Name:",
						"type": "text",
						"sticky": true,
						"disabled": true
					},
					{
						"id": "PlayerID",
						"label": "Rewards #:",
						"type": "text",
						"sticky": true,
						"disabled": true
					}
				]
			}
		],
		"submit": {
			"label": "Submit RSVP",
			"color": "#20363d"
		}
	},
	"MAIN": {
		"body_md": "# Thank you for your response! \n\nIf you have any questions or need to make changes to your RSVP, please don't hesitate to get in touch. We hope to see you soon at **Colusa Casino Resort**."
	}
}
