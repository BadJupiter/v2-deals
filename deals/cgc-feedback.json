{
	"biz_id": "jupiter",
	"check_in": true,
	"auth_required": true,
	"error_auth": {
		"title": "Woops!",
		"content": "Can't play if you ain't authenticated!"
	},
	"styling": {
		"fonts": {
			"headers": {
				"family": "Raleway",
				"weight": 700,
				"style": "italic"
			},
			"body": {
				"family": "Raleway",
				"weight": "normal"
			},
			"formlabels": {
				"family": "Raleway",
				"weight": "bold"
			}
		}
	},
	"FORM": {
		"fields": [
			{
				"type": "info",
				"justify": "center",
				"markdown": "# Your Voice Matters"
			},
			{
				"type": "info",
				"justify": "center",
				"markdown": "## Thanks for spending time with us at Celesta Grand Casino & Hotel! We'd love to hear how your visit went."
			},
			{
				"type": "info",
				"markdown": "---"
			},
			{
				"id": "games",
				"label": "What games did you play during your visit?",
				"type": "multi-select",
				"range": ["Slot Machines","Table Games","Video Poker","Bingo","Keno"],
				"required": false
			},
			{
				"id": "dining",
				"label": "What food outlets did you dine in during your visit?",
				"type": "multi-select",
				"range": ["Sterling & Slate","The Terrace Eatery","Pronto Provisions", "Perch & Perk Cafe"],
				"required": false
			},
			{
				"id": "rating",
				"label": "How was your overall experience?",
				"type": "select",
				"range": ["⭑⭑⭑⭑⭑","⭑⭑⭑⭑","⭑⭑⭑","⭑⭑","⭑"],
				"required": false
			},
			{
				"id": "comments",
				"label": "What would you like us to know?",
				"type": "textarea",
				"rows": 3,
				"required": false
			},
			{
				"id": "return",
				"label": "How likely are you to visit again?",
				"type": "select",
				"range": ["definitely will","very likely","maybe","not likely","will not"],
				"required": false
			},
						{
				"id": "amenities",
				"label": "Are there amenities we could offer to make your visit more enjoyable?",
				"type": "textarea",
				"rows": 3,
				"required": false
			},
			{
				"id": "team",
				"label": "Did anyone on our team provide you with exceptional service?",
				"type": "textarea",
				"rows": 3,
				"required": false
			},
			{
				"type": "info",
				"markdown": "---"
			},
			{
				"type": "fields_group",
				"bgcolor": "#dfe1fa",
				"fields": [
					{
						"id": "firstname",
						"label": "First Name:",
						"type": "text",
						"required": true
					},
					{
						"id": "lastname",
						"label": "Last Name:",
						"type": "text",
						"required": true
					},
					{
						"id": "email",
						"label": "Email:",
						"type": "email",
						"required": true
					}
				]
			}
		],
		"submit": {
			"label": "Submit Feedback",
			"color": "#20363d"
		},
		"notify": {
			"opt_in": "contact",
			"message": "(from {_mob}) {name} submitted a Celesta Grand feedback survey: {comments}",
			"mobiles": [
				"+15039533423"
			]
		}
	},
	"MAIN": {
		"body_md": "# Thank You!\n\nWe appreciate your feedback, and thank you for visiting Celesta Grand Casino & Hotel."
	}
}
